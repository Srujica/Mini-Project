/**
 *
 * @author LENOVO
 */
public class db {
    
    Connection conn = null;
    public static Connection java_db(){
        
        
        try{
            
            Class.forName("org.sqlite.JDBC");
            Connection conn = DriverManager.getConnection("jdbc:sqlite:C:\\Users\\LENOVO\\Documents\\NetBeansProjects\\VehicleManagement\\VehicleManagement.sqlite");
            return conn;
            }catch(Exception e)
            JOptionPane.showMessageDialog(null, e);
            return null;
        }
        
        
    }
    
}
