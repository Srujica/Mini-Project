    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
        // TODO add your handling code here:
        
          try{
            
            String sql = "Insert into Vehicle ( Vehicle_id,Vehicle_type,Vehicle_status,Availability)values(?,?,?,?)";
            pst = conn.prepareStatement(sql);
            
            pst.setString(1,txt_vehId.getText());
            pst.setString(2,txt_vehType.getText());
            pst.setString(3,txt_vehStatus.getText());
            pst.setString(4,txt_avail.getText());
            
            
            pst.execute();
                    
             JOptionPane.showMessageDialog(null, "Vehicle Added to DataBase !!");
        }catch(Exception e){
            
                        JOptionPane.showMessageDialog(null, e);
        }
        
        
    }//GEN-LAST:event_jButton1ActionPerformed

