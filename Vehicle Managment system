    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
        // TODO add your handling code here:
        
        try{
            
            String sql = "Insert into Request (Request_id,Employee_id,From_date,To_date,Request_date,Vehicle_Type,Purpose)values(?,?,?,?,?,?,?)";
            pst = conn.prepareStatement(sql);
            pst.setString(1,txt_id.getText());
            pst.setString(2,txt_id.getText());
            pst.setString(3,txt_from.getText());
            pst.setString(4,txt_to.getText());
            pst.setString(5,lbl_date.getText());
        
            
            pst.setString(6,combo_type.getSelectedItem().toString());
            pst.setString(7,txt_purpose.getText());
           
          /*  String value0  = txt_id.getText();
            String value1  = lbl_date.getText();
            String value2  = lbl_time.getText();
            
            String x = "Insert into Request(Request_id)values('"+value0+" "+value1+" "+value2+" ')";
             pst=conn.prepareStatement(x);
            */       
            pst.execute();
                    
             JOptionPane.showMessageDialog(null, "Request Sent !!!");
        }catch(Exception e){
            
            
            JOptionPane.showMessageDialog(null, e);
            
        }
        
        
    }//GEN-LAST:event_jButton1ActionPerformed

